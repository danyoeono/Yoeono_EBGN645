C o m p i l a t i o n


   1  option PROFILE=1
   2  set j "jellybeans" /yellow,blue,green,orange,purple/ ;
   3  *jellybean color
   4  set i "machines" /X1,X2/ ;
   5  *machine 1 and 2
   6  set v(i,j) "Valid combos of machines of jellybeans";
   7   
   8  alias (j, jj) ;
   9   
  10  parameters
  11  r(j) "--Net Revenue ($/bean)"
  12  /
  13  yellow 1,
  14  blue 1.05,
  15  green 1.07,
  16  orange 0.95,
  17  purple 0.9
  18  / ;
  19   
  20  *for part b-c
  21  v(i,j) = yes ;
  22   
  23  scalar
  24  hbar "--hours-- total hours in a week" /40/ ,
  25  mbar "--beans-- total amount of beans per hour" /100/ ,
  26  wkbar "--Max prod per machine per week in jellybeans" ;
  27   
  28  wkbar = hbar * mbar;
  29   
  30  *Variables
  31  positive variable X(i,j) 'Quantity of beans color j produced on machine i' ;
  32   
  33  variable Z 'Total net revenue' ;
  34   
  35   
  36  equation
  37  eq_objfn "maximize total revenue"
  38  *eq_hourlimit "no more than 40 hours per week"
  39  *eq_machinelimit "no more than 100 jb's per hour"
  40  eq_cap(i) "Machine Capacity in a week constraint" ;
  41   
  42  eq_objfn.. Z =e= sum((i,j)$v(i,j), r(j) * X(i,j)) ;
  43   
  44  eq_cap(i).. wkbar =g= sum(j$v(i,j), X(i,j));
  45   
  46  *part b
  47  model juneb /eq_objfn, eq_cap/;
  48   
  49  *part C
  50  Equations
  51      eq_prodlimit_upper(j,jj) 'Production of j is at most 1.05x production of jj',
  52      eq_prodlimit_lower(j,jj) 'Production of j is at least 0.95x production of jj';
  53   
  54  * The total production of color j must be <= 1.05 times the production of color jj
  55  eq_prodlimit_upper(j,jj).. SUM(i, X(i,j)) =l= 1.05 * SUM(i, X(i,jj));
  56   
  57  * The total production of color j must be >= 0.95 times the production of color jj
  58  eq_prodlimit_lower(j,jj).. SUM(i, X(i,j)) =g= 0.95 * SUM(i, X(i,jj));
  59   
  60  model junec /eq_objfn, eq_cap, eq_prodlimit_upper, eq_prodlimit_lower/ ;
  61   
  62  model juned /eq_objfn, eq_cap, eq_prodlimit_upper, eq_prodlimit_lower/ ;
  63   
  64  *sw_maxdev =0 ;
  65  solve juneb using lp maximizing Z;
  66   
  67  execute_unload 'juneb.gdx' ;
  68   
  69  *sw_maxdev =1 ;
  70  solve junec using lp maximizing Z;
  71   
  72  execute_unload 'junec.gdx' ;
  73   
  74  *sw_maxdev =2 ;
  75  v(i,j) = yes ;
  76  v("X1","yellow") = yes ;
  77  v("X1","blue") = yes ;
  78  v("X1","green") = yes ;
  79  v("X1","orange") = no ;
  80  v("X1","purple") = no ;
